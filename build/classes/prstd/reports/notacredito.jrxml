<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="nota" language="groovy" pageWidth="612" pageHeight="792" columnWidth="572" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="transaccion" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select nc.idnota,dc.no_documento,cl.nombre,cl.nit,nc.fecha_creacion,nc.cod_producto,p.nombre_producto
from tbl_nota_credito nc
inner join tbl_nota_transaccion nt on nc.idnota = nt.idnota
inner join tbl_documento dc on dc.idtransaccion = nt.idtransaccion
inner join tbl_nota_cliente ncl on ncl.idnota = nc.idnota
inner join tbl_cliente cl on cl.idcliente = ncl.idcliente
inner join tbl_producto p on nc.cod_producto = p.codigo
where nt.idtransaccion = $P{transaccion};]]>
	</queryString>
	<field name="idnota" class="java.lang.Integer"/>
	<field name="no_documento" class="java.lang.Integer"/>
	<field name="nombre" class="java.lang.String"/>
	<field name="nit" class="java.lang.String"/>
	<field name="fecha_creacion" class="java.sql.Timestamp"/>
	<field name="cod_producto" class="java.lang.String"/>
	<field name="nombre_producto" class="java.lang.String"/>
	<background>
		<band splitType="Stretch"/>
	</background>
	<title>
		<band height="117" splitType="Stretch"/>
	</title>
	<pageHeader>
		<band height="35" splitType="Stretch"/>
	</pageHeader>
	<columnHeader>
		<band height="61" splitType="Stretch"/>
	</columnHeader>
	<detail>
